{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "outDir": "./dist",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "server.ts"],
    "exclude": ["node_modules"]
  }