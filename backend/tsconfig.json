{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "types": ["node"],
    "outDir": "./dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts", "server.ts"],
  "exclude": ["node_modules"]
}
